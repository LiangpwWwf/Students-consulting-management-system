            android:name=".ConsultCommonQuestionActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ExpertsActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name=".MymessageActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".MyconsultActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".DraftsCollectActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".SetActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity
            android:name="com.example.hin.ui.activity.ConsultActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>
